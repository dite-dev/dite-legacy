process.title = 'dite-app-server';

(async () => {
  const config = {
    port: 3001,
  }
  const { bootstrap: createServer } = await import('{{{ serverPath }}}')
  await createServer({ config });
  const payload = {
    type: 'dite:ready',
    port: config.port,
    finishTime: Math.ceil(performance.now()),
  };
  process.send?.(payload);
})();
